# Common settings for manual work
via round ;
change drill .024 in ;
# Netlist m6809_cpu

# Exported from netlister $Revision$
Layer Top;

# Libraries
USE con-3m ;
USE ic-package ;
USE rcl ;
USE pinhead ;
USE switch ;
USE led ;
USE switch-dil ;
USE resistor-sil ;

# Component Instances
ADD PAK100/2500-50@con-3m CONN0 R0 (2.0 2.0); # idc_hdr_50w
ADD DIL40@ic-package IC_0 R0 (2.0 2.0); # mc6809
ADD DIL14@ic-package IC_2 R0 (2.0 2.0); # SN7414
ADD C050-025X075@rcl capx_0 R0 (2.0 2.0); # cap18pf
ADD C050-025X075@rcl capx_1 R0 (2.0 2.0); # cap18pf
ADD 1X03@pinhead xtal_0 R0 (2.0 2.0); # hdr1x03
ADD 1X03@pinhead lk_0 R0 (2.0 2.0); # hdr1x03
ADD 1X03@pinhead lk_1 R0 (2.0 2.0); # hdr1x03
ADD 1X04@pinhead tp_0 R0 (2.0 2.0); # hdr1x04
ADD 0204V@rcl res4k7_0 R0 (2.0 2.0); # vresistor
ADD C050-025X075@rcl cap_2 R0 (2.0 2.0); # cap100nf
ADD TL1105@switch switch_0 R0 (2.0 2.0); # TSWITCH
ADD LED3MM@led led_0 R0 (2.0 2.0); # LED3MM
ADD SD4-08@switch-dil dip4_0 R0 (2.0 2.0); # DIP8
ADD SIL9@resistor-sil sil10k_0 R0 (2.0 2.0); # r10k_sil9
ADD 0204V@rcl res100r_0 R0 (2.0 2.0); # vresistor
ADD 0204V@rcl res100r_1 R0 (2.0 2.0); # vresistor
ADD 0204V@rcl res100r_2 R0 (2.0 2.0); # vresistor
ADD 0204V@rcl res100r_3 R0 (2.0 2.0); # vresistor
ADD 0204V@rcl res100r_4 R0 (2.0 2.0); # vresistor
ADD 0204V@rcl res100r_5 R0 (2.0 2.0); # vresistor
ADD 0204V@rcl res100r_6 R0 (2.0 2.0); # vresistor
ADD 0204V@rcl res100r_7 R0 (2.0 2.0); # vresistor
ADD C050-025X075@rcl dcap_0 R0 (2.0 2.0); # cap100nf
ADD C050-025X075@rcl dcap_1 R0 (2.0 2.0); # cap100nf
ADD E2,5-5@rcl ecap_0 R0 (2.0 2.0); # cap22uf

# Define net classes
CLASS 0 default ;
CLASS 1 supply ;

# Define net connections
change class 0  ;
SIGNAL E_B_1 IC_2 2 IC_2 3;
SIGNAL A0 CONN0 33 IC_0 8;
SIGNAL RST_1 IC_2 6 IC_2 13;
SIGNAL A1 CONN0 34 IC_0 9;
SIGNAL NMI_B_PU CONN0 15 IC_0 2 sil10k_0 7;
SIGNAL A2 CONN0 35 IC_0 10;
SIGNAL A3 CONN0 36 IC_0 11;
SIGNAL A4 CONN0 37 IC_0 12;
SIGNAL A5 CONN0 38 IC_0 13;
SIGNAL A6 CONN0 39 IC_0 14;
SIGNAL HALT_B_PU CONN0 13 IC_0 40 sil10k_0 4;
SIGNAL A7 CONN0 40 IC_0 15;
SIGNAL A8 CONN0 21 IC_0 16;
SIGNAL A8SYS CONN0 41;
SIGNAL A9 CONN0 42 IC_0 17;
SIGNAL RESET_B CONN0 10 IC_0 37 IC_2 12;
SIGNAL IRQ_B CONN0 16 dip4_0 1;
SIGNAL A10 CONN0 43 IC_0 18;
SIGNAL A11 CONN0 44 IC_0 19;
SIGNAL A12 CONN0 45 IC_0 20;
SIGNAL A13 CONN0 46 IC_0 21;
SIGNAL A14 CONN0 47 IC_0 22;
SIGNAL A15 CONN0 48 IC_0 23;
SIGNAL Q_B_1 IC_2 10 IC_2 9;
SIGNAL QCLK CONN0 20 lk_1 2 tp_0 4;
SIGNAL E_0 IC_0 34 IC_2 1 lk_0 1;
SIGNAL E_2 IC_2 4 lk_0 3;

change class 1  ;
SIGNAL GND CONN0 2 CONN0 3 CONN0 4 CONN0 50 CONN0 17 CONN0 49 IC_0 1 IC_2 7 capx_0 2 capx_1 2 tp_0 1 tp_0 3 cap_2 2 switch_0 3 switch_0 4 led_0 K dcap_0 2 dcap_1 2 ecap_0 -;

change class 0  ;
SIGNAL ECLK CONN0 1 lk_0 2 tp_0 2;
SIGNAL IRQ_B_PU IC_0 3 dip4_0 16 sil10k_0 5;
SIGNAL BA CONN0 8 IC_0 6;
SIGNAL FIRQ_B_PU CONN0 23 IC_0 4 sil10k_0 6;
SIGNAL Q_0 IC_0 35 IC_2 11 lk_1 1;
SIGNAL Q_2 IC_2 8 lk_1 3;
SIGNAL LED_PU led_0 A sil10k_0 8 sil10k_0 9;
SIGNAL RST_B_0 IC_2 5 res4k7_0 2 cap_2 1 switch_0 2 switch_0 1;
SIGNAL BS CONN0 9 IC_0 5;

change class 1  ;
SIGNAL VDD CONN0 5 CONN0 24 IC_0 7 IC_2 14 res4k7_0 1 sil10k_0 1 dcap_0 1 dcap_1 1 ecap_0 +;

change class 0  ;
SIGNAL XTAL_I IC_0 39 capx_0 1 xtal_0 1;
SIGNAL CSUART_B CONN0 22;
SIGNAL XTAL_O IC_0 38 capx_1 1 xtal_0 3;
SIGNAL BREQ_B_PU CONN0 14 IC_0 33 sil10k_0 3;
SIGNAL DR1 CONN0 26 res100r_1 2;
SIGNAL DR0 CONN0 25 res100r_0 2;
SIGNAL DR3 CONN0 28 res100r_3 2;
SIGNAL DR2 CONN0 27 res100r_2 2;
SIGNAL DR5 CONN0 30 res100r_5 2;
SIGNAL DR4 CONN0 29 res100r_4 2;
SIGNAL DR7 CONN0 32 res100r_7 2;
SIGNAL DR6 CONN0 31 res100r_6 2;
SIGNAL D0 IC_0 31 res100r_0 1;
SIGNAL D1 IC_0 30 res100r_1 1;
SIGNAL D2 IC_0 29 res100r_2 1;
SIGNAL D3 IC_0 28 res100r_3 1;
SIGNAL D4 IC_0 27 res100r_4 1;
SIGNAL D5 IC_0 26 res100r_5 1;
SIGNAL D6 IC_0 25 res100r_6 1;
SIGNAL D7 IC_0 24 res100r_7 1;
SIGNAL WR_B CONN0 18 IC_0 32;
SIGNAL MRDY_PU CONN0 12 IC_0 36 sil10k_0 2;
change Thermals On ;
change Isolate 0.015 ;
change Orphans Off ;
## Maximum Eagle freeware board size is 4.0x3.2 (100x80mm)
DRC load /tmp/design_rules;

## better to work in inches for 0.1 inch pad pitch
Grid inch;
Set Wire_Bend 0;
Layer Dimension;
Wire 0  (0 0) (3.9 3.2) (0 0);
Layer Top;

## PCB-Pool/PCB-Train design rule is actually 0.006/0.006 for wires
CLASS 0 signal 0.010 0.010 ;
CLASS 1 supply 0.025 0.010 ;

## Move everything into the centre of the board before rotating
MOVE CONN0  (1.5 1.5);
MOVE IC_0  (1.5 1.5);
MOVE IC_1  (1.5 1.5);
MOVE dip4_0  (1.5 1.5);

## Rotations
ROTATE =R180 CONN0 ;
ROTATE =R270  IC_0;
ROTATE =R270  IC_1;
ROTATE =R270  dip8_0;

## Move to final positions
MOVE ecap_0         (3.70 2.40) ;
MOVE CONN0          (1.85 0.25) ;
MOVE IC_0           (0.70 1.70) ;         
MOVE capx_0         (1.20 2.5 ) ;
MOVE xtal_0         (1.20 2.4)  ;
MOVE capx_0         (1.20 2.3)  ; 
MOVE tp_0           (1.20 3.2)  ;

MOVE IC_1           (2.00 1.70) ;         
MOVE dcap_0         (0.70 2.80) ;
MOVE dcap_1         (2.00 2.80) ;
MOVE dip4_0         (3.40 2.60) ;

MOV  sil10k_0       (3.0 2.55);

MOVE switch_0        (3.4,1.0) ;
MOVE cap_2           (3.4 0.7) ;
MOVE res4k7_0        (3.4 0.4) ;
MOVE led_0           (3.4 3.0) ;

MOVE lk_0            (2.0 0.5);
MOVE lk_1            (2.0 0.7);

Layer tPlace ;
CHANGE FONT PROPORTIONAL ;
CHANGE SIZE 0.06
TEXT 'MC6809 CPU Card v1.00' R90 (0.10 0.2) ;
CHANGE SIZE 0.04
TEXT '(C) 2022 Revaldinho, github.com/revaldinho/sys6809'  R90      (0.2 0.2) ;

## WIRE  (0.1 2.91) (0.1 3.05 ) (0.78 3.05 ) (0.78 2.91) (0.1 2.91 ) ;
## TEXT 'PIO0 PIO2 PIO4 PIO6 VDD' R0 (0.13 3.00) ;
## TEXT 'GND  PIO1 PIO3 PIO5 PIO7' R0 (0.13 2.95) ;
## 
## TEXT 'GND  TP4 TP3 TP2 TP1 TP0' R90 (3.60 0.90) ;

## Preroute VDD and VSS rings and some spurs
layer bottom;
wire  0.04;
wire  'VSS' (0.04 0.04) ( 3.86 0.04) (3.86 3.16) (0.04 3.16) (0.04 0.04);
layer top;
wire  0.04;
wire  'VDD' (0.04 0.04) ( 3.86 0.04) (3.86 3.16) (0.04 3.16) (0.04 0.04);
##-- 
##-- ## Autorouter
##-- ## AUTO CLK VSS;   ## Route clock and ground first
##-- AUTO load /tmp/autorouter.ctl;
##-- AUTO ;
