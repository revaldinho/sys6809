set -tmpdir "tmp"
set -xsthdpdir "xst"
run
-ifn cpld_ram1m_plcc84.prj
-p xc9500
-ifmt mixed
-ofn cpld_ram1m_plcc84
-ofmt NGC
-top cpld_ram1m_plcc84
-opt_mode Area
-opt_level 2
-iuc NO
-lso cpld_ram1m_plcc84.lso
-keep_hierarchy No
-netlist_hierarchy as_optimized
-rtlview Yes
-hierarchy_separator /
-bus_delimiter <>
-case maintain
-verilog2001 YES
-fsm_extract YES -fsm_encoding Auto
-safe_implementation No
-mux_extract YES
-resource_sharing YES
-iobuf YES
-pld_mp YES
-pld_xp YES
-wysiwyg NO
-equivalent_register_removal YES
